msgid "tsvn:logrevregex\r\nA regular expression matching revision numbers in a log message"
msgstr "tsvn:logrevregex\nMột biểu thức thông thường khớp các số chỉnh sửa trong một thông điệp tường trình"

msgid "tsvn:logsummary\r\nA regex string which extracts a summary from a log message in its first group."
msgstr "tsvn:logsummary\nMột chuỗi regex trích xuất tóm tắt từ một thông điệp tường trình trong nhóm đầu tiên của nó."

msgid "tsvn:logtemplate\r\nString which is inserted in the log message boxes in the commit dialogs.\r\nUse this as a log message template for users to fill in."
msgstr "tsvn:logtemplate\nChuỗi được chèn trong các hộp thông điệp tường trình trong hộp thoại cam kết.\nDùng cái này như một bản mẫu thông điệp tường trình để các người dùng điền vào."

msgid "tsvn:logtemplatebranch\r\nString which is inserted in the log message boxes in the commit dialogs.\r\nUse this as a log message template for branch/tags/copies for users to fill in."
msgstr "tsvn:logtemplatebranch\nChuỗi được chèn trong các hộp thông điệp tường trình trong hộp thoại cam kết.\nDùng cái này như một bản mẫu thông điệp tường trình cho nhánh/thẻ/bản sao để các người dùng điền vào."

msgid "tsvn:logtemplatecommit\r\nString which is inserted in the log message boxes in the commit dialogs.\r\nUse this as a log message template for commits for users to fill in."
msgstr "tsvn:logtemplatecommit\nChuỗi được chèn trong các hộp thông điệp tường trình trong hộp thoại cam kết.\nDùng cái này như một bản mẫu thông điệp tường trình cho các cam kết để các người dùng điền vào."

msgid "tsvn:logtemplatedelete\r\nString which is inserted in the log message boxes in the commit dialogs.\r\nUse this as a log message template for deletes for users to fill in."
msgstr "tsvn:logtemplatedelete\nChuỗi được chèn trong các hộp thông điệp tường trình trong hộp thoại cam kết.\nDùng cái này như một bản mẫu thông điệp tường trình cho việc xóa để các người dùng điền vào."

msgid "tsvn:logtemplateimport\r\nString which is inserted in the log message boxes in the commit dialogs.\r\nUse this as a log message template for imports for users to fill in."
msgstr "tsvn:logtemplateimport\nChuỗi được chèn trong các hộp thông điệp tường trình trong hộp thoại cam kết.\nDùng cái này như một bản mẫu thông điệp tường trình cho việc nhập khẩu để các người dùng điền vào."

msgid "tsvn:logtemplatelock\r\nString which is inserted in the log message boxes in the get-lock dialogs.\r\nUse this as a log message template for get-locks for users to fill in."
msgstr "tsvn:logtemplatelock\nChuỗi được chèn trong các hộp thông điệp tường trình trong hộp thoại lấy khóa.\nDùng cái này như một bản mẫu thông điệp tường trình cho việc lấy khóa để các người dùng điền vào."

msgid "tsvn:logtemplatemkdir\r\nString which is inserted in the log message boxes in the commit dialogs.\r\nUse this as a log message template for directory creations for users to fill in."
msgstr "tsvn:logtemplatemkdir\nChuỗi được chèn trong các hộp thông điệp tường trình trong hộp thoại lấy khóa.\nDùng cái này như một bản mẫu thông điệp tường trình cho việc tạo thư mục để các người dùng điền vào."

msgid "tsvn:logtemplatemove\r\nString which is inserted in the log message boxes in the commit dialogs.\r\nUse this as a log message template for moves/renames for users to fill in."
msgstr "tsvn:logtemplatemove\nChuỗi được chèn trong các hộp thông điệp tường trình trong hộp thoại lấy khóa.\nDùng cái này như một bản mẫu thông điệp tường trình cho việc di chuyển/đổi tên để các người dùng điền vào."

msgid "tsvn:logtemplatepropset\r\nString which is inserted in the log message boxes in the commit dialogs.\r\nUse this as a log message template for property modifications for users to fill in."
msgstr "tsvn:logtemplatepropset\nChuỗi được chèn trong các hộp thông điệp tường trình trong hộp thoại lấy khóa.\nDùng cái này như một bản mẫu thông điệp tường trình cho việc sửa đổi thuộc tính để các người dùng điền vào."

msgid "tsvn:logwidthmarker\r\nSet this to the number of chars the log message should not exceed its width.\r\nA faded line is shown in the commit dialogs at that position. The default is 80 chars."
msgstr "tsvn:logwidthmarker\nĐặt nó về số lượng ký tự mà thông điệp tường trình không nên vượt quá chiều rộng của nó.\nMột dòng mờ đi được hiển thị trong hộp thoại cam kết tại vị trí này. Mặc định là 80 ký tự."

msgid "tsvn:projectlanguage\r\nSet this to the language number you want your log messages written in.\r\nTortoiseSVN will then load the appropriate spell checker module if available."
msgstr "tsvn:projectlanguage\nĐặt nó thành số ngôn ngữ mà bạn muốn thông điệp tường trình của bạn được viết.\nTortoiseSVN sẽ tải bộ kiểm tra lỗi chính tả phù hợp nếu tìm được."

msgid "tsvn:warnifnoissue\r\nSet to 'yes' if a warning shall be shown when no issue is entered in the commit dialog."
msgstr "tsvn:warnifnoissue\nĐặt thành 'có' nếu một cảnh báo phải được hiển thị khi không có vấn đề nào được nhập vào trong hộp thoại cam kết."

